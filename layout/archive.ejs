<div>
  <% var max_num = Number(theme.max_num.Archives) %>
  <p><%- max_num %></p>
</div>

<div>
  <% var i = 0; site.posts.forEach(function (post) { %>
    <% i += 1 %>
    <p><%- i %></p>
    <% if (i < theme.max_num.Archives) { %>
      <p><%= date(post.date, "YYYY-MM-DD") %></p>
      <a href="<%- url_for(post.path) %>"><%= post.title %></a>
    <% } %>
  <% }) %>
<div>

<%- partial('partial/paginator.ejs') %>